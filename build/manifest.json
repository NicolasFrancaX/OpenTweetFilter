{
  "manifest_version": 2,
  "name": "Open Tweet Filter",
  "version": "2.2.2",
  "description": "__MSG_description__",
  "update_url":"http://clients2.google.com/service/update2/crx",
  "content_scripts": [ {
    "js": [ 
      "jquery/jquery-1.7.1.min.js",
      "jquery/jquery-ui-1.8.16.custom.min.js",
      "jquery/tipsy/tipsy.js", 
      "ko/knockout-2.1.0.js",
      "ko/knockout.localStorage-latest.js",
      "coffeekup/coffeekup.js",
      "filter.js"
    ],
        "css": [
            "filter.css"
        ],
    "matches": [ "http://twitter.com/*", "https://twitter.com/*" ]
  } ],
  "web_accessible_resources": [
    "jquery/tipsy/tipsy.css",
    "jquery/tipsy/tipsy.gif"
  ],
  "icons": {
    "128": "icon.png"
  },      
  "default_locale": "en"
}
